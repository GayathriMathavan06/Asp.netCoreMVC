@model MVCCoreDemo.Models.Student

@{
    ViewData["Title"] = "Student Details";
}

<h2>Details</h2>

<div>
    <h4>Student</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>Name</dt>
        <dd>@Model.Name</dd>
        <dt>Gender</dt>
        <dd>@Model.Gender</dd>
        <dt>Department</dt>
        <dd>@Model.Department</dd>
        <dt>City</dt>
        <dd>@Model.City</dd>
    </dl>
</div>

<div>
    <a asp-action="Edit" asp-route-id="@Model.StudId">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
